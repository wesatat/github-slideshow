{
  "expo": {
    "name": "aurora plus seed diary",
    "slug": "aurora-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.auroraplus.aurorplusseeddiary"
    },
    "android": {
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-t.png"   
      },
      "package": "com.aurora.auroraplus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
